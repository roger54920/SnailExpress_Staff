<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/ll_content"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:paddingBottom="@dimen/px_20"
    android:paddingRight="@dimen/px_32"
    android:layout_marginTop="@dimen/px_20"
    android:paddingLeft="@dimen/px_32"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/shipperCode"
        android:text="承运来源：EMS"
        android:layout_marginTop="@dimen/px_25"
        android:textSize="@dimen/px_28"
        android:textColor="@color/cl_333333"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:text="快递单号：等数据"
        android:layout_marginTop="@dimen/px_15"
        android:textSize="@dimen/px_28"
        android:textColor="@color/cl_333333"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/lgisticcode" />

    <TextView
        android:id="@+id/createDate"
        android:text="2016/12/18 11:00"
        android:layout_marginTop="@dimen/px_15"
        android:textSize="@dimen/px_28"
        android:textColor="@color/cl_333333"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/send_operation_procedure"
        android:text="111"
        android:layout_marginTop="@dimen/px_15"
        android:textSize="@dimen/px_28"
        android:textColor="@color/cl_333333"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <RelativeLayout
        android:layout_marginTop="@dimen/px_40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/sir_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="李先生"
            android:textColor="@color/cl_333333"
            android:textSize="@dimen/px_30" />

        <TextView
            android:singleLine="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/px_40"
            android:layout_toRightOf="@+id/sir_name"
            android:gravity="left"
            android:id="@+id/sir_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="北京市朝阳区望京花家北理1等数据"
            android:textColor="@color/cl_666666"
            android:textSize="@dimen/px_28" />
    </RelativeLayout>
    <TextView
        android:id="@+id/send_courier_service_parcel_state"
        android:text="11111111"
        android:layout_marginTop="@dimen/px_15"
        android:textSize="@dimen/px_28"
        android:textColor="@color/cl_6fd1c8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</LinearLayout>